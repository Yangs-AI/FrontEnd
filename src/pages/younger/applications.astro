---
import Layout from '../../layouts/Layout.astro';
import Container from '../../layouts/Container.astro';
import Prose from '../../layouts/modules/Prose.astro';
import Breadcrumbs from '../../layouts/modules/Breadcrumbs.astro';

import SMarkdown from '../../components/SMarkdown.svelte';

const breadcrumbs = [
  {
    name: "Younger",
    path: "/younger",
  },
]

---
<Layout title='Applications - Younger'>
  <Container>
    <Prose>
      <Breadcrumbs breadcrumbs={breadcrumbs} classname="not-prose pl-0" current_loc='Applications'/>
      <SMarkdown singletonName={'YoungerApplications'} client:load/>
    </Prose>
  </Container>
</Layout>