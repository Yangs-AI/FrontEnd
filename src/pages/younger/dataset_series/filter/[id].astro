---
import Layout from '../../../../layouts/Layout.astro';
import Container from '../../../../layouts/Container.astro';
import Breadcrumbs from '../../../../layouts/modules/Breadcrumbs.astro';

const { id } = Astro.params;

const breadcrumbs = [
  {
    name: "Younger",
    path: "/younger",
  },
  {
    name: "Dataset Series",
    path: "/younger/dataset_series",
  },
  {
    name: "Filter",
    path: "/younger/dataset_series/filter",
  },
]

---

<Layout title='Instance - Filter Series'>
  <Container>
    <Breadcrumbs breadcrumbs={breadcrumbs} current_loc={"Instance ID - "+ id}/>
  </Container>
</Layout>