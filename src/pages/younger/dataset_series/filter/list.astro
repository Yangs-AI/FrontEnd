---
import Layout from '../../../../layouts/Layout.astro';
import Container from '../../../../layouts/Container.astro';
import Breadcrumbs from '../../../../layouts/modules/Breadcrumbs.astro';

import SYFilter from '../../../../components/younger/SYFilter.svelte';

const breadcrumbs = [
  {
    name: "Younger",
    path: "/younger",
  },
  {
    name: "Dataset Series",
    path: "/younger/dataset_series",
  },
  {
    name: "Filter",
    path: "/younger/dataset_series/filter",
  },
]

---

<Layout title='Filter Series - Younger'>
  <Container>
    <Breadcrumbs breadcrumbs={breadcrumbs} current_loc='Instances'/>
    <SYFilter client:load/>
  </Container>
</Layout>
